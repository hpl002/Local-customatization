{
  "title": "commands for non-apple keyboards",
  "rules": [
    {
      "description": "command+r by f5 in non-Apple keyboards",
      "manipulators": [
        {
          "conditions": [
            {
              "type": "device_unless",
              "identifiers": [
                {
                  "vendor_id": 1452
                }
              ]
            }
          ],
          "type": "basic",
          "from": {
            "key_code": "f5",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "r",
          "modifiers": ["command"]
            }
          ]
        }
      ]
    },
    {
